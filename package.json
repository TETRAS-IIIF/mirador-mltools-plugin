{
  "name": "mirador-mltools-plugin",
  "version": "0.0.5",
  "description": "mirador-mltools-plugin React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run build:es",
    "build:umd": "NODE_ENV=production webpack --mode=production",
    "build:es": "mkdir -p es && cp -r src/* es && NODE_ENV=production MODULE_FORMAT=es npx babel es -d es",
    "build:cjs": "mkdir -p lib && cp -r src/* lib && NODE_ENV=production MODULE_FORMAT=cjs npx babel lib -d lib",
    "build:demo": "NODE_ENV=development webpack --mode=development",
    "clean": "rm -rf ./umd && rm -rf ./es && rm -rf ./lib && rm -rf ./demo/dist",
    "clean-deps": "rm -rf package-lock.json && rm -rf node_modules",
    "lint": "eslint ./src ./__tests__",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build:es",
    "start": "NODE_ENV=development webpack serve --open",
    "test": "npm run lint && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --watchAll=false"
  },
  "peerDependencies": {
    "@mui/material": "^6.x",
    "@mui/styles": "^6.x",
    "mirador": "^4.0.0-alpha.5",
    "prop-types": "^15.7.2",
    "react": "18.x"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@mui/material": "^6.3.0",
    "@mui/styles": "^6.x",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mirador": "^4.0.0-alpha.5",
    "prop-types": "^15.7.2",
    "react": "18.x",
    "react-dom": "18.x",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "author": "",
  "homepage": "",
  "license": "Apache-2.0",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.0"
  }
}
